    "cross-fetch": "^3.1.4",
    "flutterwave-node-v3": "^1.0.8",
    "handlebars": "^4.7.7",
    "juice": "^8.0.0",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "koa-router": "^10.1.1",
